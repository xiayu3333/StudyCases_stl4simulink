    <bind component="curves" as="curves_1" x="431.0" y="370.0" width="116.0" height="42.0">
      <map key="N_u">1.8</map>
      <map key="event">event</map>
      <map key="N_l">0.8</map>
      <map key="P">1</map>
      <map key="eps">0</map>
    </bind>
  </component>
</sspaceex>

